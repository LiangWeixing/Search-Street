/* page/register/register.wxss */
@import "../../weui.wxss";
.header{
  height: 90px;
}
.userinfo-avatar {
  overflow:hidden;
  display: block;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  background-color: #fff;
  margin-left: 120px;
  position: absolute;
  margin-top: 5px;
}
.reghd-text{
  text-align: left;
  font-size: 16px;
}
.reg-hd{
  height: 210px;
  background-color: #34d4dae0;
}
.section-sttext{
  font-size: 17px;
  color: #132627b0;
  padding-left: 15px;
}
.section-stimage{
  height: 120px;
}
.store-locat{
  font-size: 17px;
  color: #000;
  background-color: #34d4dae0;
  text-align: center;
  padding: 10px;
}
.big-logo{
  height: 80px;
  width: 80px;
  position: absolute;
}
.logoinfo{
  height: 80px;
  width: 80px;
  position: absolute;
  z-index: 2;
}
.big-logof{
  display: none;
}
.slocat-img{
  height: 40px;
  width: 40px;
  float:left;
}
.slocat-text{
  border: 1px solid #132627b0;
  border-radius: 5px;
  float:left;
  width: 70%;
  height: 60rpx;
  margin-top: 5px;
}
.check{
  width: 320rpx;
  height: 80rpx;
  line-height: 60rpx;
  background: #F7B45D;
  color: white;
  border: none;
  position: absolute;
  margin-top: 20px;
  margin-left: 100px;
}
.submit-append{
  font-size: 15px;
  color: #132627b0;
  text-align: center;
  position: absolute;
  margin-top: 60px;
  margin-left: 100px;
}
